meta {
  name: Join House By Code
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/houses/join
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "code": "{{houseCode}}",
    "userId": "{{joiningUserId}}"
  }
}
